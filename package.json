{
	"name": "@ctx-core/auth0",
	"version": "27.0.2",
	"description": "ctx-core auth0",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file -e auth0_body_,password_realm_body_"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.34",
		"@ctx-core/combinators": "workspace:^8.0.27",
		"@ctx-core/dom": "workspace:^11.0.41",
		"@ctx-core/email": "workspace:^9.0.27",
		"@ctx-core/env": "workspace:^14.0.2",
		"@ctx-core/error": "workspace:^11.1.10",
		"@ctx-core/fetch": "workspace:^11.0.94",
		"@ctx-core/function": "workspace:^20.2.13",
		"@ctx-core/jwt": "workspace:^10.0.39",
		"@ctx-core/local-storage": "workspace:^10.0.29",
		"@ctx-core/object": "workspace:^22.0.1",
		"@ctx-core/store": "workspace:^27.0.48",
		"auth0-js": "^9.18.0",
		"deep-equal": "^2.0.5",
		"jsonwebtoken": "^8.5.1",
		"svelte": "^3.44.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/auth0": "^2.34.8",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
