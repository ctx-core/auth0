{
	"name": "@ctx-core/auth0",
	"version": "25.0.75",
	"description": "ctx-core auth0",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file -e auth0_body_,password_realm_body_"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.18",
		"@ctx-core/combinators": "workspace:^8.0.14",
		"@ctx-core/dom": "workspace:^11.0.22",
		"@ctx-core/email": "workspace:^9.0.14",
		"@ctx-core/env": "workspace:^13.0.34",
		"@ctx-core/error": "workspace:^11.0.15",
		"@ctx-core/fetch": "workspace:^11.0.60",
		"@ctx-core/function": "workspace:^20.1.3",
		"@ctx-core/jwt": "workspace:^10.0.21",
		"@ctx-core/local-storage": "workspace:^10.0.15",
		"@ctx-core/object": "workspace:^21.0.17",
		"@ctx-core/package": "workspace:^9.0.15",
		"@ctx-core/store": "workspace:^27.0.25",
		"auth0-js": "^9.16.4",
		"deep-equal": "^2.0.5",
		"esm": "^3.2.25",
		"jsonwebtoken": "^8.5.1",
		"svelte": "3.42.4"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.1.2",
		"@types/auth0": "^2.33.5",
		"typescript": "^4.4.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}