{
	"name": "@ctx-core/auth0",
	"version": "28.0.6",
	"description": "ctx-core auth0",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file -e auth0_body_,password_realm_body_"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.1.0",
		"@ctx-core/combinators": "workspace:^8.0.34",
		"@ctx-core/dom": "workspace:^11.1.0",
		"@ctx-core/email": "workspace:^9.0.34",
		"@ctx-core/env": "workspace:^15.0.5",
		"@ctx-core/error": "workspace:^11.1.17",
		"@ctx-core/fetch": "workspace:^11.0.102",
		"@ctx-core/function": "workspace:^20.2.20",
		"@ctx-core/jwt": "workspace:^10.0.46",
		"@ctx-core/local-storage": "workspace:^10.0.36",
		"@ctx-core/nanostores": "workspace:^0.3.0",
		"@ctx-core/object": "workspace:^22.0.8",
		"auth0-js": "^9.18.0",
		"deep-equal": "^2.0.5",
		"jsonwebtoken": "^8.5.1",
		"svelte": "^3.44.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.123",
		"@types/auth0": "^2.34.9",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js"
}