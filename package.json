{
	"name": "@ctx-core/auth0",
	"version": "24.0.9",
	"description": "ctx-core auth0",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file -e auth0_body_,password_realm_body_"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^22.0.4",
		"@ctx-core/combinators": "workspace:^7.0.2",
		"@ctx-core/dom": "workspace:^10.0.3",
		"@ctx-core/email": "workspace:^8.0.2",
		"@ctx-core/env": "workspace:^12.0.6",
		"@ctx-core/error": "workspace:^10.0.2",
		"@ctx-core/fetch": "workspace:^10.0.4",
		"@ctx-core/function": "workspace:^19.1.2",
		"@ctx-core/jwt": "workspace:^9.0.3",
		"@ctx-core/local-storage": "workspace:^9.0.2",
		"@ctx-core/object": "workspace:^20.0.3",
		"@ctx-core/package": "workspace:^8.0.2",
		"@ctx-core/store": "workspace:^26.0.4",
		"auth0-js": "^9.16.2",
		"deep-equal": "^2.0.5",
		"jsonwebtoken": "^8.5.1",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^6.0.3",
		"@types/auth0": "^2.33.3",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}