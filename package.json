{
	"name": "@ctx-core/auth0",
	"version": "24.0.51",
	"description": "ctx-core auth0",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file -e auth0_body_,password_realm_body_"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^22.2.0",
		"@ctx-core/combinators": "workspace:^7.0.10",
		"@ctx-core/dom": "workspace:^10.0.26",
		"@ctx-core/email": "workspace:^8.0.10",
		"@ctx-core/env": "workspace:^12.0.29",
		"@ctx-core/error": "workspace:^10.1.9",
		"@ctx-core/fetch": "workspace:^10.0.39",
		"@ctx-core/function": "workspace:^19.2.0",
		"@ctx-core/jwt": "workspace:^9.0.17",
		"@ctx-core/local-storage": "workspace:^9.0.17",
		"@ctx-core/object": "workspace:^20.4.2",
		"@ctx-core/package": "workspace:^8.0.16",
		"@ctx-core/store": "workspace:^26.0.27",
		"auth0-js": "^9.16.2",
		"deep-equal": "^2.0.5",
		"jsonwebtoken": "^8.5.1",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^6.0.16",
		"@types/auth0": "^2.33.4",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}