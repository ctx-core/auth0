{
	"name": "@ctx-core/auth0",
	"version": "26.0.5",
	"description": "ctx-core auth0",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file -e auth0_body_,password_realm_body_"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.27",
		"@ctx-core/combinators": "workspace:^8.0.21",
		"@ctx-core/dom": "workspace:^11.0.34",
		"@ctx-core/email": "workspace:^9.0.21",
		"@ctx-core/env": "workspace:^13.0.49",
		"@ctx-core/error": "workspace:^11.1.3",
		"@ctx-core/fetch": "workspace:^11.0.85",
		"@ctx-core/function": "workspace:^20.2.7",
		"@ctx-core/jwt": "workspace:^10.0.32",
		"@ctx-core/local-storage": "workspace:^10.0.22",
		"@ctx-core/object": "workspace:^21.0.26",
		"@ctx-core/package": "workspace:^9.1.4",
		"@ctx-core/store": "workspace:^27.0.40",
		"auth0-js": "^9.18.0",
		"deep-equal": "^2.0.5",
		"jsonwebtoken": "^8.5.1",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.2.4",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.113",
		"@types/auth0": "^2.34.7",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}